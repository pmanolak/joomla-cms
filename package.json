    "@vue/compiler-sfc": "^3.5.22",
    "chokidar": "^4.0.3",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.1",
    "core-js": "^3.45.1",
    "cypress": "^15.3.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.5.0",
    "fs-extra": "^11.3.2",
    "ini": "^5.0.0",
    "jasmine-core": "^5.10.0",
    "joomla-cypress": "^1.3.1",
    "lightningcss": "^1.30.1",
    "mysql": "^2.18.1",
    "pg": "^8.16.3",
    "postcss-scss": "^4.0.9",
    "recursive-readdir": "^2.2.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.2",
    "rollup-plugin-vue": "^6.0.0",
    "rtlcss": "^4.3.0",
    "sass-embedded": "^1.93.2",
    "semver": "^7.7.2",
    "smtp-tester": "^2.1.0",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.12.1",
    "totp-generator": "1.0.0"
  }
}
